{
  "name": "remiz-editor",
  "version": "0.0.4",
  "description": "GUI editor for remiz game engine",
  "main": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "bin": {
    "remiz-editor": "./bin/index.js"
  },
  "scripts": {
    "start": "env NODE_ENV=development node './bin/index.js' --config 'fixture/remiz-editor.config.js'",
    "build": "npm run build:clean && npm run build:app && npm run build:esm && npm run build:types",
    "build:clean": "rm -rf build && rm -rf esm",
    "build:app": "env NODE_ENV=production webpack --config webpack.config.js",
    "build:esm": "npx babel src --out-dir esm --extensions \".ts,.js\" --ignore \"src/**/*.test.ts,src/**/*.test.tsx\"",
    "build:types": "tsc --project tsconfig.types.json",
    "lint": "eslint .",
    "test": "jest",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "bin",
    "build",
    "esm",
    "electron",
    "LICENSE",
    "index.js",
    "preload.js",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michailRemmele/remiz-editor.git"
  },
  "author": "Mikhail Remmele",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michailRemmele/remiz-editor/issues"
  },
  "homepage": "https://github.com/michailRemmele/remiz-editor#readme",
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.10",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@emotion/babel-plugin": "11.11.0",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@types/jest": "27.5.0",
    "@types/lodash.isequal": "^4.5.6",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "antd": "5.5.2",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "4.0.0",
